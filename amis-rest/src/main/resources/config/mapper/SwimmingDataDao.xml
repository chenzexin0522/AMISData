<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.amis.dao.SwimmingDataDao" >



    <insert id="insertSwimmingData">
        insert into swimming_data
        (
        mac,
        data
         )
        value
        (
        #{mac},
        #{data}
        )
    </insert>



    <select id="getSwimmingDataExcel" resultType="com.amis.entity.MotionDataEntity">
        select mac,data from swimming_data
    </select>


    <update id="truncateTable">
        truncate table swimming_data
    </update>


</mapper>